        "build-test": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration test",
        "build-test-stats": "node --max_old_space_size=6144 ./node_modules/@angular/cli/bin/ng build --configuration test --stats-json",
        "publish-test": "npm run build-test && firebase use test && firebase deploy",
        "test": "ng test",
        "lint": "ng lint",
        "e2e": "ng e2e",
        "bundle-report": "webpack-bundle-analyzer dist/stats.json"
